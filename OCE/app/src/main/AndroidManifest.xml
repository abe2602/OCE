<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abe.mobiletrabalho">
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:configChanges="orientation|screenSize|keyboardHidden" android:name=".camera.CameraActivity" />
        <activity android:configChanges="orientation|screenSize|keyboardHidden" android:name=".map.MapActivity" />
        <activity android:configChanges="orientation|screenSize|keyboardHidden" android:name=".danger.DangerActivity" />
        <activity android:configChanges="orientation|screenSize|keyboardHidden" android:name=".mic.MicActivity" />
        <activity android:configChanges="orientation|screenSize|keyboardHidden" android:name=".vibra.VibraActivity" />
        <activity android:configChanges="orientation|screenSize|keyboardHidden" android:name=".emotions.EmotionsActivity"/>
    </application>
</manifest>